{
	"name": "volt",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"lint": "next lint",
		"format": "prettier --write .",
		"build": "next build",
		"build.local": "yarn test && next build",
		"start": "next start",
		"dev": "next dev",
		"test": "vitest run",
		"test.watch": "vitest",
		"generate.module": "plop",
		"release.prepare": "changeset && git add . && echo Release prepared and staged, please commit right away and continue working on your changes, run `yarn release.version` to version the release once all changes are staged.",
		"release.version": "changeset version && git add . && echo 'Release versioned and staged, please commit right away and open a PR to wrap-up the release.'"
	},
	"dependencies": {
		"@changesets/cli": "^2.27.7",
		"@emotion/cache": "^11.13.1",
		"@emotion/react": "^11.13.0",
		"@emotion/server": "^11.11.0",
		"@emotion/styled": "^11.13.0",
		"@hookform/resolvers": "^3.9.0",
		"@mui/icons-material": "^5.16.6",
		"@mui/material": "^5.16.6",
		"@mui/material-nextjs": "^5.16.4",
		"@mui/x-date-pickers": "^7.11.1",
		"@uidotdev/usehooks": "^2.4.1",
		"axios": "^1.7.2",
		"change-case": "^5.4.4",
		"clsx": "^2.1.1",
		"luxon": "^3.4.4",
		"lz-string": "^1.5.0",
		"next": "14.2.5",
		"prop-types": "^15.8.1",
		"react": "^18",
		"react-dom": "^18",
		"react-hook-form": "^7.52.1",
		"react-query": "^3.39.3",
		"react-toastify": "^10.0.5",
		"tailwind-merge": "^2.4.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@testing-library/react": "^15.0.7",
		"@vitejs/plugin-react": "^4.2.1",
		"eslint": "8.19.0",
		"jsdom": "^24.0.0",
		"plop": "^4.0.1",
		"postcss": "^8.4.14",
		"tailwindcss": "^3.4.1",
		"vitest": "^1.6.0"
	}
}
